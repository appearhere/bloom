{
  "name": "@appearhere/bloom-styleguide",
  "version": "1.1.0-beta.66",
  "private": true,
  "dependencies": {
    "@appearhere/bloom": "^1.1.0-beta.66",
    "@appearhere/common-styles": "^1.1.0-beta.61",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0-next.9754a231",
    "refractor": "^2.3.0",
    "rehype": "^5.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "dev": "yarn start"
  },
  "devDependencies": {
    "eslint": "4.15.0",
    "raw-loader": "^0.5.1",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewired": "^1.5.0",
    "react-hot-loader": "^4.0.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "jest": {
    "displayName": "styleguide",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|json|md)$": "<rootDir>../../__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>../../__mocks__/cssMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>../../node_modules/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>../../scripts/setupTest.js"
  }
}
