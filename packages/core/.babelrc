{
  "env": {  
    "development": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-regenerator", {
          "async": false
        }],
        ["@babel/plugin-transform-runtime", {
          "helpers": false
        }],
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-object-rest-spread",
        "inline-react-svg"
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-regenerator", {
          "async": false
        }],
        ["@babel/plugin-transform-runtime", {
          "helpers": false
        }],
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-object-rest-spread",
        "inline-react-svg"
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-regenerator", {
          "async": false
        }],
        ["@babel/plugin-transform-runtime", {
          "helpers": false
        }],
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-object-rest-spread",
        "inline-react-svg"
      ]
    }
  }
}
