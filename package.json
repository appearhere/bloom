{
  "name": "@appearhere/bloom",
  "version": "0.23.0",
  "description": "Appear Here's pattern library and styleguide",
  "keywords": [
    "Pattern library",
    "styleguide",
    "Appear Here",
    "react",
    "components"
  ],
  "homepage": "https://github.com/appearhere/bloom",
  "repository": {
    "type": "git",
    "url": "https://github.com/appearhere/bloom.git"
  },
  "contributors": [
    "Richard Palmer <rich@rdjpalmer.com> (http://rdjpalmer.com)",
    "Nick Maher <nick@appearhere.co.uk>",
    "Daniel Magliola <daniel@appearhere.co.uk>",
    "Nique Woodhouse <nique@appearhere.co.uk>"
  ],
  "engines": {
    "node": ">=6.5.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.14.0",
    "@kadira/storybook-deployer": "^1.2.0",
    "autoprefixer": "6.4.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-regenerator": "6.14.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-latest": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-runtime": "6.11.6",
    "case-sensitive-paths-webpack-plugin": "1.1.3",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.0",
    "css-loader": "0.24.0",
    "detect-port": "1.0.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-config-airbnb-base": "^7.1.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "http-proxy-middleware": "0.17.1",
    "http-server": "^0.9.0",
    "jest": "^16.0.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "opn": "4.0.2",
    "path-exists": "3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-loader": "0.11.1",
    "promise": "7.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "recursive-readdir": "2.0.0",
    "rimraf": "2.5.4",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "svgo": "^0.7.0",
    "svgo-loader": "^1.1.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "1.15.1",
    "webpack-postcss-tools": "^1.1.2",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "array-from": "^2.1.1",
    "classnames": "^2.2.5",
    "commonmark": "^0.26.0",
    "commonmark-react-renderer": "^4.3.1",
    "exenv": "^1.2.1",
    "invariant": "^2.2.1",
    "lodash": "^4.16.3",
    "lost": "^7.1.0",
    "object-fit-images": "^2.5.9",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-container-query": "^0.6.0",
    "react-dom": "^15.3.1",
    "react-html5video": "^1.2.12",
    "react-motion": "^0.4.5",
    "react-on-visible": "^1.0.3",
    "react-stickynode": "^1.2.1",
    "subscribe-ui-event": "^1.0.14",
    "warning": "^3.0.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "heroku-postbuild": "build-storybook",
    "lint": "esw -c ./config/eslint.js ./components",
    "lint:watch": "esw -w -c ./config/eslint.js ./components",
    "postpublish": "yarn run deploy-storybook",
    "prepublish": "yarn test",
    "serve-storybook": "http-server ./storybook-static -p $PORT",
    "start": "node scripts/start.js",
    "storybook": "start-storybook -p 9009",
    "test": "jest --env=jsdom",
    "test:travis": "jest --env=jsdom --coverage",
    "test:watch": "jest --watch --env=jsdom"
  },
  "eslintConfig": {
    "extends": "./config/eslint.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/config/jest/FileStub.js",
      "^[./a-zA-Z0-9$_\\-\\!]+\\.css": "<rootDir>/config/jest/CSSStub.js"
    },
    "scriptPreprocessor": "<rootDir>/config/jest/transform.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTestFramework.js",
    "coverageDirectory": "<rootDir>/coverage"
  }
}
