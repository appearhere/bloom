  "scripts": {
    "build": "node scripts/build.js",
    "build-storybook": "build-storybook",
    "heroku-postbuild": "yarn build && yarn run build-storybook;",
    "lint": "esw -c ./config/eslint.js ./components ./styleguide",
    "lint:watch": "esw -w -c ./config/eslint.js ./components ./styleguide",
    "serve": "node ./server",
    "start": "node scripts/start.js",
    "storybook": "start-storybook -p 9009",
    "test": "jest --env=jsdom --verbose",
    "test:travis": "jest --env=jsdom --coverage",
    "test:watch": "jest --watch --notify --env=jsdom"
  },
  "eslintConfig": {
    "extends": "./config/eslint.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_!-]+\\.(jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|json|md)$": "<rootDir>/config/jest/FileStub.js",
      "^[./a-zA-Z0-9$_\\-\\!]+\\.css": "<rootDir>/config/jest/CSSStub.js"
    },
    "transform": {
      ".*": "<rootDir>/config/jest/transform.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "raf/polyfill"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTestFramework.js",
    "coverageDirectory": "<rootDir>/coverage"
  }
}
